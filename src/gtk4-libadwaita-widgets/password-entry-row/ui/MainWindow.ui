        <property name="margin-start">12</property>
        <property name="spacing">12</property>
        <child>
          <object class="AdwPasswordEntryRow"></object>
        </child>
      </object>
    </child>
  </template>
  <menu id="primary_menu">
    <section>
      <item>
        <attribute name="label" translatable="true">PreferÃªncias</attribute>
